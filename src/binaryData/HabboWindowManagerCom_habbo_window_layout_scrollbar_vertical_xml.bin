		</scrollbar_slider_button_up>
		
		<scrollbar_slider_track_vertical name="slider_track" x="0" y="16" width="17" height="24" tags="_EXCLUDE, _INTERNAL, slider_track">
			<params>
				<param name="input_event_processor" />
				<param name="relative_horizontal_scale_fixed" />
				<param name="relative_vertical_scale_strech" />
				<param name="use_parent_graphic_context" />
			</params>
			<children>
