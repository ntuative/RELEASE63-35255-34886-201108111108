<?xml version="1.0"?>
<layout name="habbo_window_layout_scrollbar_vertical" width="17" height="56" version="0.0">

	<window>
	
		<scrollbar_slider_button_up name="decrement" x="0" y="0" width="17" height="16" tags="_EXCLUDE, _INTERNAL, decrement">
			<params>
				<param name="input_event_processor" />
				<param name="relative_horizontal_scale_fixed" />
				<param name="relative_vertical_scale_fixed" />
				<param name="use_parent_graphic_context" />
